{
  "name": "knex-tdd-tutorial",
  "version": "1.0.0",
  "description": "Knex TDD Tutorial",
  "main": "index.js",
  "author": "Rob Porter <rob@weeverapps.com>",
  "license": "MIT",
  "scripts": {
    "build": "babel --presets es2015 src -d dist",
    "start": "node dist/index.js",
    "unit": "find ./src -name '*.spec.js' | ./node_modules/cross-env/bin/cross-env.js NODE_ENV=unit xargs ./node_modules/mocha/bin/mocha --require babel-register",
    "integration": "npm run build && npm run migrate:latest && ./node_modules/cross-env/bin/cross-env.js NODE_ENV=development ./node_modules/mocha/bin/mocha test/integration --require babel-register",
    "init": "knex init",
    "migrate:make": "knex migrate:make",
    "migrate:latest": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "seed:make": "knex seed:make",
    "seed:run": "knex seed:run"
  },
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "bluebird": "^3.4.6",
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "cross-env": "^3.1.3",
    "eslint": "^3.11.1",
    "hippie": "^0.5.1",
    "knex": "^0.12.6",
    "mocha": "^3.2.0",
    "mock-knex": "^0.3.7",
    "pg": "^6.1.0",
    "restify": "^4.3.0"
  }
}
